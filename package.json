	"ntl": {
		"descriptions": {
			"start": "View menu",
			"menu:close": "Close this menu -> Do nothing",
			"info": "NPM info about this package.json scripts",
			"menu": "Open interactive script menu"
		}
	},
	"scripts": {
		"start": "ntl",
		"menu:close": "",
		"info": "npm run",

		"test": "jest --config=./tests/jest.config.ts",
		"test:watch": "nodemon --ext js,jsx,env,ts --watch \"*.*\" --exec \"npm run test\"",
		"test:output-coverage": "jest --config=./tests/jest.config.ts --collectCoverage",

		"build:ts": "npx tsc",
		"build:ts:watch": "nodemon --ext js,jsx,env,ts --watch \"*.*\" --exec \"npm run build:ts\"",
		"build": "webpack",
		"dev:lint": "eslint ./ --ignore-path .gitignore",
		"dev:lint:fix": "eslint ./ --ignore-path .gitignore --fix",
		"dev:lint:log": "eslint ./ --ignore-path .gitignore --output-file eslint.html --format compact",
		"dev:lint:watch": "nodemon --ext js,jsx,md,ts --watch \"*.*\" --exec \"npm run dev:lint\"",
		"dev:format": "prettier --write src/**/*.{js,jsx",
		"dev:format:watch": "nodemon --ext js,jsx,md,ts --watch \"*.*\" --exec \"npm run dev:lint\"",

		"docs:build": "jsdoc2md -t README.hbs src/*.js > README.md",
		"docs:build:watch": "nodemon --config ./scripts/dev-server.nodeamon.json --exec \"npm run docs:build\"",

		"menu": "ntl"
	}
