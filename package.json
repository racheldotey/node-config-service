{
	"name": "node-config-service",
	"version": "1.0.0",
	"private": false,
	"keywords": [
		"config",
		"node",
		"env",
		"environment",
		"system",
		"process",
		"settings",
		"dotenv",
		"backend",
		"front-end"
	],
	"description": "A config management service for your next node app.",
	"author": {
		"name": "Rachel Dotey",
		"email": "hello@racheldotey.com",
		"url": "https://racheldotey.ninja"
	},
	"license": "MIT",
	"homepage": "https://github.com/racheldotey/node-config-service",
	"repository": {
		"type": "git",
		"url": "https://github.com/racheldotey/node-config-service"
	},
	"bugs": {
		"url": "https://github.com/racheldotey/node-config-service/issues"
	},
	"main": "./src/index.js",
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	],
	"engines": {
		"node": ">= 6.0.0",
		"npm": ">= 3.0.0"
	},
	"dependencies": {
		"dotenv": "^16.0.2",
		"dotenv-expand": "^9.0.0",
		"path": "^0.12.7"
	},
	"devDependencies": {
		"clean-jsdoc-theme": "^4.1.8",
		"concurrently": "^7.4.0",
		"connect-livereload": "^0.6.1",
		"express": "^4.18.1",
		"jsdoc": "^3.6.11",
		"jsdoc-to-markdown": "^7.1.1",
		"livereload": "^0.9.3",
		"nodemon": "^2.0.20",
		"ntl": "^5.1.0",
		"open-cli": "^7.0.1",
		"prettier": "^2.7.1"
	},
	"scripts": {
		"start": "ntl",
		"menu": "ntl",
		"info": "npm run",
		"test": "bash ./scripts/run-tests.sh",
		"format": "prettier --write src/**/*.{js,jsx",
		"build": "bash ./scripts/build-dist.sh && npm run docs-build",
		"build-docs": "bash ./scripts/build-docs.sh",
		"serve": "nodemon ./scripts/dev-server.js",
		"develop": "npm run test && npm run build && npm run serve",
		"view-docs-in-browser": "open-cli http://localhost:5555/"
	}
}
