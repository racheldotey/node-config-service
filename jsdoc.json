{
	"sourceType": "module",
	"source": {
		"include": ["src", "README.md", "package.json"]
	},
	"plugins": ["plugins/markdown"],
	"markdown": {
		"hardwrap": false,
		"idInHeadings": true
	},
	"opts": {
		"encoding": "utf8",
		"readme": "README.md",
		"destination": "docs/jsdoc",
		"recurse": true,
		"verbose": true,
		"template": "node_modules/clean-jsdoc-theme",
		"theme_opts": {
			"base_url": "/",
			"homepageTitle": "NodeConfigService Docs",
			"default_theme": "dark"
		}
	}
}
